        "created": { "type": "datetime", "required": true },
        "updated": { "type": "datetime", "required": true },
        "expired": { "type": "datetime" }
    },
    "relations": {
        "Index": { "local": "interface", "foreign": "interface", "type": "many", "className": "Tecnodesign_Studio_Index" }
